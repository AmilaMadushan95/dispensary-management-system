      </children>
   </AnchorPane>



</center>
<top>
   <HBox prefHeight="55.0" prefWidth="1579.0" BorderPane.alignment="CENTER">
      <children>
         <AnchorPane prefHeight="55.0" prefWidth="1586.0">
            <children>
               <Pane fx:id="topPane" layoutX="306.0" layoutY="-1.0" prefHeight="55.0" prefWidth="1273.0" stylesheets="@stylesheet.css">
                  <children>
                     <Label layoutX="957.0" layoutY="8.0" text="Dispensary Management System" textFill="#cdcdcd">
                        <font>
                           <Font name="Brush Script MT Italic" size="28.0" />
                        </font>
                     </Label>
                  </children>
               </Pane>
               <Pane fx:id="labicon" prefHeight="55.0" prefWidth="307.0" stylesheets="@stylesheet.css">
                  <children>
                     <Label layoutX="84.0" layoutY="7.0" text="Laboratory" textFill="WHITE">
                        <graphic>
                           <FontAwesomeIconView glyphName="FLASK" size="40" />
                        </graphic>
                        <font>
                           <Font name="Segoe UI Light Italic" size="21.0" />
                        </font>
                     </Label>
                  </children>
               </Pane>
            </children></AnchorPane>
      </children>
   </HBox>
</top>
   <left>
      <VBox fx:id="aside" alignment="CENTER" prefHeight="715.0" prefWidth="308.0" stylesheets="@stylesheet.css" BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="dashboardBT" contentDisplay="TOP" onMouseClicked="#changePanel" prefHeight="163.0" prefWidth="308.0" stylesheets="@stylesheet.css" text="Dash Board" textFill="WHITE">
               <graphic>
                  <MaterialDesignIconView glyphName="VIEW_MODULE" size="80" />
               </graphic></JFXButton>
            <JFXButton fx:id="ordertestBT" contentDisplay="TOP" onMouseClicked="#changePanel" prefHeight="163.0" prefWidth="308.0" stylesheets="@stylesheet.css" text=" Order Test" textFill="WHITE">
               <graphic>
                  <FontAwesomeIconView glyphName="LIST_ALT" size="70" />
               </graphic></JFXButton>
            <JFXButton fx:id="enter_resultBT" contentDisplay="TOP" onMouseClicked="#changePanel" prefHeight="163.0" prefWidth="415.0" stylesheets="@stylesheet.css" text="Enter Results" textFill="WHITE">
               <graphic>
                  <FontAwesomeIconView glyphName="EDIT" size="80" />
               </graphic></JFXButton>
            <JFXButton fx:id="viewDB_BT" contentDisplay="TOP" onMouseClicked="#changePanel" prefHeight="163.0" prefWidth="415.0" stylesheets="@stylesheet.css" text="View Database" textFill="WHITE">
               <graphic>
                  <FontAwesomeIconView glyphName="DATABASE" size="70" />
               </graphic></JFXButton>
            <JFXButton fx:id="extraBT" contentDisplay="TOP" onMouseClicked="#changePanel" prefHeight="163.0" prefWidth="415.0" stylesheets="@stylesheet.css" text="Extras" textFill="WHITE">
               <graphic>
                  <FontAwesomeIconView glyphName="PIE_CHART" size="70" />
               </graphic></JFXButton>
         </children>
      </VBox>
   </left>
</BorderPane>
