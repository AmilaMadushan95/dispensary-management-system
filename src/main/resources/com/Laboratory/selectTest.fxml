            <content>
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                     <TableView prefHeight="315.0" prefWidth="390.0">
                        <columns>
                           <TableColumn prefWidth="178.0" text="Main Tests" />
                           <TableColumn prefWidth="211.0" text="Sub Tests" />
                        </columns>
                     </TableView>
                     <Button layoutX="284.0" layoutY="340.0" mnemonicParsing="false" text="Remove" />
                  </children>
               </AnchorPane>
            </content>
         </TitledPane>
         <Button layoutX="448.0" layoutY="526.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="116.0" text="Add" />
