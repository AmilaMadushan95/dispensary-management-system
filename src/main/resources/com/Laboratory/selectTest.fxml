                           <TableColumn prefWidth="91.0" text="Test ID" />
                           <TableColumn prefWidth="238.0" text="Test Name" />
                        </columns>
                     </TableView>
                     <TextField layoutX="30.0" layoutY="32.0" prefHeight="38.0" prefWidth="275.0" promptText="Search test" />
                  </children>
               </AnchorPane>
            </content>
         </TitledPane>
         <TitledPane animated="false" collapsible="false" layoutX="379.0" layoutY="138.0" prefHeight="370.0" prefWidth="253.0" text="Sub Tests">
            <content>
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                     <TableView prefHeight="326.0" prefWidth="249.0">
                        <columns>
                           <TableColumn prefWidth="249.0" text="Sub Test Names" />
                        </columns>
                     </TableView>
                  </children>
               </AnchorPane>
            </content>
         </TitledPane>
         <JFXButton layoutX="985.0" layoutY="18.0" prefHeight="58.0" prefWidth="143.0" stylesheets="@stylesheet.css" text="Done" />
